{
  "version": "1.2.0",
  "name": "metalsmith-imagemin",
  "description": "Metalsmith plugin to minify images.",
  "author": "Ahmad Nassri <ahmad@ahmadnassri.com> (https://www.ahmadnassri.com/)",
  "homepage": "https://github.com/ahmadnassri/metalsmith-imagemin",
  "repository": "ahmadnassri/metalsmith-imagemin",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "compress",
    "gif",
    "imagemin",
    "images",
    "jpeg",
    "jpg",
    "metalsmith",
    "png"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/ahmadnassri/metalsmith-imagemin/issues"
  },
  "scripts": {
    "pretest": "standard && echint",
    "test": "mocha",
    "posttest": "npm run coverage",
    "coverage": "istanbul cover --dir coverage _mocha -- -R dot",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "echint": {
    "ignore": [
      "test/**/*[.jpg,.png,.gif,.ico]",
      "coverage/**"
    ]
  },
  "devDependencies": {
    "codeclimate-test-reporter": "0.3.2",
    "echint": "^1.5.0",
    "istanbul": "^0.3.18",
    "mocha": "^2.2.5",
    "standard": "^7.1.2"
  },
  "dependencies": {
    "async": "^1.4.2",
    "debug-log": "^1.0.0",
    "imagemin": "^5.0.2"
  }
}
